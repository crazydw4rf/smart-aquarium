{
  "css.lint.emptyRules": "ignore",
  "editor.fontFamily": "ZedMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "window.zoomLevel": 1,
  "editor.fontWeight": "600",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "[typescript]": {
    "editor.inlayHints.enabled": "off"
  },
  "workbench.view.showQuietly": {
    "workbench.panel.output": true
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "cmath": "cpp",
    "numeric": "cpp",
    "ostream": "cpp",
    "streambuf": "cpp",
    "utility": "cpp",
    "cstring": "cpp",
    "string_view": "cpp",
    "ranges": "cpp",
    "istream": "cpp"
  },
  "C_Cpp.errorSquiggles": "enabled",
  "workbench.colorCustomizations": {
    "editorWarning.foreground": "#00000000"
  }
}